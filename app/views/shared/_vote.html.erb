<% if feed_item.user.private_followable = true && !current_user.following?(feed_item.user) %>
	<% if feed_item.voted_by?(current_user) %>
		<% if current_user.voted_for?(feed_item) %>
			<%= link_to unvote_story_path(feed_item), class: "wir" do %><i class="icon-wir" style= "background-position: -16px -16px"></i><% end %><%= render partial: "shared/votecount", locals: {feed_item: feed_item} %><%= link_to vote_down_story_path(feed_item), class: "wiw" do %>
				<i class="icon-wiw"></i><% end %>
		<% else %>
			<%= link_to vote_up_story_path(feed_item), class: "wir" do %><i class="icon-wir"></i><% end %><%= render partial: "shared/votecount", locals: {feed_item: feed_item} %><%= link_to unvote_story_path(feed_item), class: "wiw" do %><i class="icon-wiw" style= "background-position: -16px 0px"></i><% end %>
			<% end %>
	<% else %>
		<%= link_to vote_up_story_path(feed_item), class: "wir" do %><i class="icon-wir"></i><% end %><%= render partial: "shared/votecount", locals: {feed_item: feed_item} %><%= link_to vote_down_story_path(feed_item), class: "wiw" do %><i class="icon-wiw"></i><% end %>
	<% end %>

<% else %>
	<% if feed_item.voted_by?(current_user) %>
		<% if current_user.voted_for?(feed_item) %>
			<%= link_to unvote_story_path(feed_item), class: "wir", remote: true do %><i class="icon-wir" style= "background-position: -16px -16px"></i><% end %><%= render partial: "shared/votecount", locals: {feed_item: feed_item} %><%= link_to vote_down_story_path(feed_item), class: "wiw", remote: true do %>
				<i class="icon-wiw"></i><% end %>
		<% else %>
			<%= link_to vote_up_story_path(feed_item), class: "wir", remote: true do %><i class="icon-wir"></i><% end %><%= render partial: "shared/votecount", locals: {feed_item: feed_item} %><%= link_to unvote_story_path(feed_item), class: "wiw", remote: true do %><i class="icon-wiw" style= "background-position: -16px 0px"></i><% end %>
			<% end %>
	<% else %>
		<%= link_to vote_up_story_path(feed_item), class: "wir", remote: true do %><i class="icon-wir"></i><% end %><%= render partial: "shared/votecount", locals: {feed_item: feed_item} %><%= link_to vote_down_story_path(feed_item), class: "wiw", remote: true do %><i class="icon-wiw"></i><% end %>
	<% end %>
<% end %>